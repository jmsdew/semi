        <mapper resource="mapper/student/studentMapper.xml"/>
        <mapper resource="mapper/student/topRankMapper.xml"/>
        <mapper resource="mapper/student/sitDownMapper.xml"/>
        <mapper resource="mapper/student/praiseMapper.xml"/>
        <mapper resource="mapper/student/attendanceMapper.xml"/>
        <mapper resource="mapper/student/gradeMapper.xml"/>
        <mapper resource="mapper/student/calendarMapper.xml"/>
    </mappers>

</configuration>
